import std
 struct P2[A, B](a: A, b: B)

foreign fn newArray[A](): Array[A]

fn swap[A, B](p: P2[A, B]) {
    var pp = p;
    let x = pp.a;
    pp.a = pp.b;
    pp.b = x;
}

 fn main() {
    let a = P2("a", 100);
    let b = P2(200, "b");
    swap(a);
    swap(b);
 }